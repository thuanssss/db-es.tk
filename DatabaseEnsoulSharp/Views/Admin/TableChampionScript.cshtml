@model List<DatabaseEnsoulSharp.Models.Database.ChampionScript>
<div>
    <table id="TableList" class="display table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>Image</th>
                <th>Name Champion</th>
                <th>Author</th>
                <th>Status </th>
            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.Id</td>
                <td>
                    <img class="card-img-top"
                         style="width: 20px; height: 20px;"
                         src="http://ddragon.leagueoflegends.com/cdn/9.11.1/img/champion/@(item.Name).png"
                         alt="@item.Name" />
                </td>
                <td>@item.Champion.Name</td>
                <td>@item.ScriptInfo.Author</td>
                <td>@item.Status</td>
            </tr>
            }
        </tbody>
    </table>
</div>