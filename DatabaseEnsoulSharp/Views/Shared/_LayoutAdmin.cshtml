<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <title>LobiAdmin</title>
    <link rel="shortcut icon" href="/images/logo/lobiadmin-logo-16.ico" />

    <link rel="stylesheet" href="~/lobibox/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/lobibox/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/lobibox/css/weather-icons.min.css" />
    <link rel="stylesheet" href="~/lobibox/css/lobiadmin-with-plugins.css" />
    <link rel="stylesheet" href="~/lobibox/css/demo.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@8/dist/sweetalert2.min.css">

    @RenderSection("Styles", required: false)

    <script type="text/javascript" src="~/lobibox/js/lib/jquery.min.js"></script>
    <script type="text/javascript" src="~/lobibox/js/bootstrap/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/lobibox/js/lobi-plugins/lobibox.min.js"></script>
    <script type="text/javascript" src="~/lobibox/js/lobi-plugins/lobipanel.min.js"></script>
    <script type="text/javascript" src="~/lobibox/js/config.js"></script>
    <script type="text/javascript" src="~/lobibox/js/LobiAdmin.js"></script>
    <script type="text/javascript" src="~/lobibox/js/app.js"></script>
    <script type="text/javascript" src="~/lobibox/js/demo.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/sweetalert2@8"></script>
    <script src="~/lobibox/js/custom/app.js"></script>

    <style>
        body {
            background: #ececec;
        }
    </style>
</head>
<body>
    <nav class="navbar navbar-default navbar-header header">
        <a class="navbar-brand" href="#">
            <div class="navbar-brand-img"></div>
        </a>
        <ul class="nav navbar-nav pull-left show-hide-menu">
            <li>
                <a href="#" class="border-radius-0 btn font-size-lg" data-action="show-hide-sidebar">
                    <i class="fa fa-bars"></i>
                </a>
            </li>
        </ul>
    </nav>
    <div class="menu">
        <nav>
            <ul>
                <li>
                    <a id="clickChampion" href="javascript:;">
                        <i class="fa fa-share-alt menu-item-icon"></i>
                        <span class="inner-text">Champion</span>
                    </a>
                </li>
                <li>
                    <a id="clickScriptInfo" href="javascript:;">
                        <i class="fa fa-share-alt menu-item-icon"></i>
                        <span class="inner-text">Script Info</span>
                    </a>
                </li>
                <li>
                    <a id="clickChampionScript" href="javascript:;">
                        <i class="fa fa-share-alt menu-item-icon"></i>
                        <span class="inner-text">Champion Script</span>
                    </a>
                </li>
                
            </ul>
        </nav>
        <div class="menu-collapse-line">
            <div class="menu-toggle-btn" data-action="collapse-expand-sidebar"></div>
        </div>
    </div>

    <div id="main">
        <div id="content">
            @RenderBody()
        </div>
    </div>
    <div class="spinner spinner-horizontal hide">
        <span class="spinner-text">Loading...</span>
        <div class="bounce1"></div>
        <div class="bounce2"></div>
        <div class="bounce3"></div>
    </div>

    @RenderSection("Scripts", required: false)
</body>
</html>